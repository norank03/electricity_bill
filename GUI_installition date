    private void signmeActionPerformed(java.awt.event.ActionEvent evt) {                                       
      try
       {
      if(signme==evt.getSource())
      {  
        New_customer o  = new New_customer();
        int i ;
      
        int b=o.insert( i=Integer.parseInt(id11.getText()),name1.getText(), password1.getText(), region1.getText(), address1.getText(), email1.getText(),contract1.getText());
        
        TheEmail g=new TheEmail();
        g.print.setText("installition will be in the date below");
        g.etext.setText(o.toString(b));
        setVisible(false);
     g.setVisible(true);
          
      }
       }
       
       catch(Exception xcp)
       {
           
           JOptionPane.showMessageDialog(null,xcp.getMessage(),"Error",JOptionPane.ERROR_MESSAGE);
       }
    }                                      

